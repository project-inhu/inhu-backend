{
  "name": "inhu",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "seed": "tsx --env-file=.env DB/seed.ts",
    "seed:prod": "node dist/DB/seed.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm run test:infra:up && dotenv -e .env.test -- jest --config ./test/jest-e2e.json && npm run test:infra:down",
    "test:infra:up": "docker compose -f docker-compose.test.yml up -d",
    "test:infra:down": "docker compose -f docker-compose.test.yml down -v",
    "dev:infra:up": "docker compose up -d",
    "dev:infra:down": "docker compose down -v",
    "dev:infra:connect": "docker exec -it inhu-postgres psql -d inhu -U inhu_admin"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.846.0",
    "@aws-sdk/s3-request-presigner": "^3.846.0",
    "@faker-js/faker": "^9.9.0",
    "@nestjs-cls/transactional": "^3.1.0",
    "@nestjs-cls/transactional-adapter-prisma": "^1.3.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/common": "^11.1.4",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.4",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.1.4",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.12.0",
    "axios": "^1.10.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "dotenv-cli": "^8.0.0",
    "ioredis": "^5.6.1",
    "jwks-rsa": "^3.2.0",
    "nestjs-cls": "^6.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@chax-at/transactional-prisma-testing": "^1.3.1",
    "@faker-js/faker": "^9.9.0",
    "@golevelup/ts-jest": "^0.7.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.4",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.14",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "jest": "^30.0.4",
    "prettier": "^3.6.2",
    "prisma": "^6.12.0",
    "prisma-case-format": "^2.2.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
